{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "settings": {
  }
}
